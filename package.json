{
	"name": "labonair-connectivity",
	"displayName": "Labonair Connectivity",
	"description": "Connectivity features for Labonair",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.85.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:labonair.views.hosts"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "labonair",
					"title": "Connectivity",
					"icon": "$(server-process)"
				}
			]
		},
		"views": {
			"labonair": [
				{
					"id": "labonair.views.hosts",
					"name": "Host Manager",
					"type": "webview"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"dependencies": {
		"clsx": "^2.1.0",
		"lucide-react": "^0.300.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"uuid": "^9.0.1",
		"ssh2": "^1.15.0"
	},
	"devDependencies": {
		"@types/node": "18.x",
		"@types/react": "^18.2.45",
		"@types/react-dom": "^18.2.18",
		"@types/uuid": "^9.0.7",
		"@types/vscode": "^1.85.0",
		"@typescript-eslint/eslint-plugin": "^6.15.0",
		"@typescript-eslint/parser": "^6.15.0",
		"css-loader": "^6.8.1",
		"eslint": "^8.56.0",
		"style-loader": "^3.3.3",
		"ts-loader": "^9.5.1",
		"typescript": "^5.3.3",
		"webpack": "^5.89.0",
		"webpack-cli": "^5.1.4"
	}
}
