{
  "name": "terminus",
  "private": true,
  "version": "1.2.0",
  "description": "A web-based server management platform with SSH terminal, tunneling, and file editing capabilities",
  "author": "Snenjih",
  "main": "electron/main.cjs",
  "type": "module",
  "scripts": {
    "clean": "npx prettier . --write",
    "cleanup": "node -e \"process.platform === 'win32' ? require('child_process').execSync('scripts\\\\cleanup-electron.bat') : require('child_process').execSync('bash scripts/cleanup-electron.sh')\"",
    "dev": "vite",
    "build": "vite build && tsc -p tsconfig.node.json",
    "build:backend": "tsc -p tsconfig.node.json",
    "dev:backend": "tsc -p tsconfig.node.json && node ./dist/backend/backend/starter.js",
    "dev:all": "concurrently --names \"BACKEND,FRONTEND\" --prefix-colors \"bgBlue.bold,bgMagenta.bold\" \"npm run dev:backend\" \"wait-on http://localhost:30001 && npm run dev\"",
    "preview": "vite preview",
    "electron:dev": "npm run cleanup && npm run build:backend && concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "build:win-portable": "npm run build && electron-builder --win --dir --publish=never",
    "build:win-installer": "npm run build && electron-builder --win --publish=never",
    "build:linux-portable": "npm run build && electron-builder --linux --dir --publish=never",
    "build:linux-appimage": "npm run build && electron-builder --linux AppImage --publish=never",
    "build:linux-targz": "npm run build && electron-builder --linux tar.gz --publish=never",
    "test:encryption": "tsc -p tsconfig.node.json && node ./dist/backend/backend/utils/encryption-test.js",
    "migrate:encryption": "tsc -p tsconfig.node.json && node ./dist/backend/backend/utils/encryption-migration.js",
    "build:dmg": "npm run build && electron-builder --mac dmg --publish=never",
    "build:signed": "npm run build:dmg",
    "build:all": "npm run build && electron-builder --mac --linux --win --publish=never",
    "build:vscode": "npm run build:backend && npm run build:vscode:frontend && npm run build:vscode:extension",
    "build:vscode:frontend": "vite build --outDir vscode-extension/dist/frontend",
    "build:vscode:extension": "cd vscode-extension && npm install && npm run compile",
    "watch:vscode": "cd vscode-extension && npm run watch",
    "package:vscode": "cd vscode-extension && npm run package",
    "dev:vscode": "npm run build:backend && concurrently \"npm run watch:vscode\" \"code --extensionDevelopmentPath=${PWD}/vscode-extension\""
  },
